var listRecommendedTutor = function(restURL, num, divId) {
	$.ajax({
		type : "POST",
		url : "${pageContext.servletContext.contextPath}" + restURL,
		success : function(data) {
			var html = '';
			for ( var i = 0; i < data.length; i++ ) {
				html += '<li>\n';
				html += '	<a href="test.html" alt="' + data[i].tutorName + '">\n';
				html += '		<img class="img-circle" src="' + data[i].profileImagePath + '">\n';
				html += '	</a>\n';
				html += '</li>\n';
			}
			
			$("#" + divId).html(html);
			
			//thumb gallery
			$('#img_galley').flexslider({
				animation: 'slide', 
	            controlNav: true,
	            directionNav: true,
	            animationLoop: false,
	            slideshow: false,
	            minItems: 5,
	            move: 1,
	            itemWidth: 100,
	            itemMargin: 15,
				asNavFor: '#img_thumb'
			});
		},
		dataType : "json"
	});
};
