<script>
$(document).ready(function() {
	var frstPageNo = $z.util.getFirstPageNo(1, 7);
	
	listMainRecentVideo(frstPageNo);
	listRecommendedVideo(frstPageNo);
	listPopularVideo(frstPageNo);
	
	listRecommendedPlaylist(frstPageNo);
	
	// listRecommendedTutor("/tutor/list/recommended", 10, "recommended-tutor-ul");
	
	bindMorePage();
	
	
	
	// 메인 광고
	$.ajax({
		type : "POST",
		url : "${pageContext.servletContext.contextPath}" + "/advertise/main",
		success : function(data) {
			$('#ad-main-banner-tutor').attr("src", data.profileImagePath);
			$('#ad-main-banner-tutor-bg').attr("src", "https://" + data.bannerImagePath);
			$('#ad-main-banner-link').attr("href", data.adSite);
			$('#ad-main-banner-link').attr("alt", "" + data.adSiteTitle);
			$('#ad-main-banner-link').attr("title", "" + data.adSiteTitle);
		},
		dataType : "json"
	});
	
	// 사이 광고
	$.ajax({
		type : "POST",
		url : "${pageContext.servletContext.contextPath}/advertise/side?size=5",
		success : function(data) {
			var html = "";
			for ( var i = 0; i < data.length; i++ ) {
				html += '<div class="row" style="margin-bottom: 5px;">\n';
				html += '	<div class="col-xs-4" style="margin-right: -15px;">\n';
				html += '		<a href="' + $z.link.getTutorDetail("${pageContext.servletContext.contextPath}", data[i].tutorId) + '">\n';
				html += '		    <img class="img img-circle" style="width: 100%;" src="' + data[i].profileImagePath + '">\n';
				html += '		</a>\n';
				html += '	</div>\n';
				html += '	<div class="col-xs-8">\n';
				//html += '		<a href="' + $z.link.getTutorDetail("${pageContext.servletContext.contextPath}", data[i].tutorId) + '">\n';
				//html += '			<p class="z-tutor-name-text">' + data[i].tutorName + '</p>\n';
				//html += '		</a>\n';
				html += '       <p style="padding-top: 15px;">\n';
				html += '			<a href="' + $z.link.getTutorDetail("${pageContext.servletContext.contextPath}", data[i].tutorId) + '" class="z-tutor-name-text">\n';
				html += '				' + data[i].tutorName + '\n';
				html += '			</a>\n';
				html += '       <\p>\n';
				html += '	</div>\n';
				html += '</div>\n';
			}
			
			$('#ad-side-banner-tutor').html(html);
		},
		dataType : "json"
	});
	
	
});
</script>

